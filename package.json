        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "cpx": "^1.5.0",
        "gulp": "^4.0.0",
        "gulp-babel": "^7.0.1",
        "gulp-concat": "^2.6.1",
        "gulp-jshint": "^2.1.0",
        "gulp-ng-annotate": "^2.1.0",
        "gulp-uglify": "^3.0.2",
        "gulp-util": "^3.0.8",
